<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>BlockedApps</key>
	<array>
		<dict>
			<key>Application</key>
			<string>com.apple.InstallAssistant.Catalina</string>
			<key>DeleteApp</key>
			<false />
